function pos_ee_C = RotationWorldToContactFrame (base_quat, pos_ee_W)

% It assumes flat terrain for now
n = size(base_quat,1);

for i = 1:n
 w_R_b = quat2rotm(base_quat(n,:));
 n = [0; 0; 1];
 heading = w_R_b * [1; 0; 0];
 ey = cross(n, heading);
 ex = cross(ey, n);
 ez = n;
 
 c_R_w = []
 
end

end